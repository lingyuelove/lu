    <!--获取上传下载任务列表-->
    <select id="listVoOpUploadDownload" resultType="com.luxuryadmin.vo.op.VoOpUploadDownload">
        SELECT
          oud.id,
          oud.state,
          oud.name,
          oud.total_ms 'totalMs',
          oud.download_times 'downloadTimes',
          oud.type,
          oud.module,
          oud.start_time 'startTime',
          oud.end_time 'endTime',
          oud.insert_time 'insertTime',
          oud.finish_time 'finishTime',
          oud.update_time 'updateTime',
          oud.remark,
          oud.url,
          ref.`name` 'nickname',
          ref2.`name` 'updateNickname'
        FROM
          op_upload_download oud
          LEFT JOIN shp_user_shop_ref ref
            ON oud.`fk_shp_user_id` = ref.`fk_shp_user_id`
            AND ref.`fk_shp_shop_id` = #{shopId}
            LEFT JOIN shp_user_shop_ref ref2
            ON oud.`fk_shp_user_id` = ref2.`fk_shp_user_id`
            AND ref2.`fk_shp_shop_id` = #{shopId}
        WHERE oud.`fk_shp_shop_id` = #{shopId}
        ORDER BY id DESC
  </select>
