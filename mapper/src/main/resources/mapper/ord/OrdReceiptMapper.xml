    <!--根据订单编号或去订单凭证-->
    <select id="getOrdReceiptByOrderNumber" resultType="com.luxuryadmin.vo.ord.VoOrdReceipt">
        SELECT
          re.id 'id',
          re.fk_ord_order_number 'orderBizId',
          re.party_a 'partyA',
          re.NAME 'proName',
          re.pro_unique_code 'uniqueCode',
          re.num 'totalNum',
          re.total_price 'totalPrice',
          DATE_FORMAT(od.`finish_time`, '%Y-%m-%d') 'insertTime',
          re.address,
          re.party_b 'partyB',
          re.phone,
          re.state
        FROM
          ord_receipt re
          LEFT JOIN ord_order od
            ON re.`fk_ord_order_id` = od.`id`
        WHERE re.fk_shp_shop_id = #{shopId}
          AND re.fk_ord_order_number = #{orderBizId}
    </select>
    <delete id="deleteObject" parameterType="java.lang.Integer">
    delete from ord_receipt
    where id = #{id,jdbcType=INTEGER}
  </delete>

    <!--根据订单编号删除订单凭证;订单编号用in查询-->
